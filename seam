#/bin/sh
############################################
#
# seam-gen
#
############################################

cd seam-gen

if [ "${1}" = new-project ] 
then

  ant new-project ${3} ${4} ${5} ${6} ${7} -Dproject.name=${2} 

elif [ "${1}" = new-wtp-project ] 
then

  ant new-wtp-project ${3} ${4} ${5} ${6} ${7} -Dproject.name=${2}

else

   ant ${1} ${2} ${3} ${4} ${5} ${6} ${7}

fi

cd ..