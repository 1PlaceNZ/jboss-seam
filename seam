#/bin/sh
############################################
#
# seam-gen
#
############################################

cd seam-gen

if [ "${1}" = help ]
then
   cat README:
else
   java -cp ${JAVA_HOME}/lib/tools.jar:../lib/ant-nodeps.jar:../lib/ant-launcher.jar:../lib/ant.jar:../ -Dant.home=../lib org.apache.tools.ant.launch.Launcher ${1} ${2} ${3} ${4} ${5} ${6} ${7}
fi

cd ..