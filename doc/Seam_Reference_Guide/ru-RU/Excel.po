#. Tag: para
#: Excel.xml:2374
#, no-c-format
msgid "Provides cell merging"
msgstr ""

#. Tag: literal
#: Excel.xml:2383
#, no-c-format
msgid "&lt;e:mergeCells&gt;"
msgstr ""

#. Tag: para
#: Excel.xml:2392
#, no-c-format
