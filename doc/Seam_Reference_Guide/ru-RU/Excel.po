#: Excel.xml:2448
#, no-c-format
msgid ""
"<![CDATA[\r\n"
"               <e:workbook>\r\n"
"                  <e:worksheet>\r\n"
"                     <e:mergeCells startRow=\"0\" startColumn=\"0\" endRow="
"\"9\" endColumn=\"9\"/>\r\n"
"                  </e:worksheet>\r\n"
"               </e:workbook>            \r\n"
"            ]]>"
