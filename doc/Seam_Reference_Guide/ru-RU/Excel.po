#. Tag: title
#: Excel.xml:1730
#, no-c-format
msgid "Headers and footers"
msgstr ""

#. Tag: para
#: Excel.xml:1731
#, no-c-format
msgid ""
"Headers and footers are childrens of worksheets and contain facets which in "
"turn contains a string with commands that are parsed."
msgstr ""

#. Tag: literal
#: Excel.xml:1743
#, no-c-format
msgid "&lt;e:header&gt;"
msgstr ""

#. Tag: para
#: Excel.xml:1772 Excel.xml:1835
#, no-c-format
msgid ""
"<literal>left</literal> &#8212;The contents of the left header/footer part."
msgstr ""

#. Tag: para
#: Excel.xml:1779 Excel.xml:1842
#, no-c-format
msgid ""
"<literal>center</literal> &#8212;The contents of the center header/footer "
"part."
msgstr ""

#. Tag: para
#: Excel.xml:1786 Excel.xml:1849
#, no-c-format
msgid ""
"<literal>right</literal> &#8212;The contents of the right header/footer part."
msgstr ""

#. Tag: literal
#: Excel.xml:1806
#, no-c-format
msgid "&lt;e:footer&gt;"
msgstr ""

#. Tag: para
#: Excel.xml:1861
#, no-c-format
msgid ""
"The content of the facets is a string that can contain various #-delimited "
"commands as follows:"
msgstr ""

#. Tag: para
#: Excel.xml:1872
#, no-c-format
msgid "#date#"
msgstr ""

#. Tag: para
#: Excel.xml:1875
#, no-c-format
msgid "Inserts the current date"
msgstr ""

#. Tag: para
#: Excel.xml:1880
#, no-c-format
msgid "#page_number#"
msgstr ""

#. Tag: para
#: Excel.xml:1883
#, no-c-format
msgid "Inserts the current page number"
msgstr ""

#. Tag: para
#: Excel.xml:1888
#, no-c-format
msgid "#time#"
msgstr ""

#. Tag: para
#: Excel.xml:1891
#, no-c-format
msgid "Inserts the current time"
msgstr ""

#. Tag: para
#: Excel.xml:1896
#, no-c-format
msgid "#total_pages#"
msgstr ""

#. Tag: para
#: Excel.xml:1899
#, no-c-format
msgid "Inserts the total page count"
msgstr ""

#. Tag: para
#: Excel.xml:1904
#, no-c-format
msgid "#worksheet_name#"
msgstr ""

#. Tag: para
#: Excel.xml:1907
#, no-c-format
msgid "Inserts the worksheet name"
msgstr ""

#. Tag: para
#: Excel.xml:1912
#, no-c-format
msgid "#workbook_name#"
msgstr ""

#. Tag: para
#: Excel.xml:1915
#, no-c-format
msgid "Inserts the workbook name"
msgstr ""

#. Tag: para
#: Excel.xml:1920
#, no-c-format
msgid "#bold#"
msgstr ""

#. Tag: para
#: Excel.xml:1923
#, no-c-format
msgid "Toggles bold font, use another #bold# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:1931
#, no-c-format
msgid "#italics#"
msgstr ""

#. Tag: para
#: Excel.xml:1934
#, no-c-format
msgid "Toggles italic font, use another #italic# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:1942
#, no-c-format
msgid "#underline#"
msgstr ""

#. Tag: para
#: Excel.xml:1945
#, no-c-format
msgid "Toggles underlining, use another #underline# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:1953
#, no-c-format
msgid "#double_underline#"
msgstr ""

#. Tag: para
#: Excel.xml:1956
#, no-c-format
msgid ""
"Toggles double underlining, use another #double_underline# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:1964
#, no-c-format
msgid "#outline#"
msgstr ""

#. Tag: para
#: Excel.xml:1967
#, no-c-format
msgid "Toggles outlined font, use another #outline# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:1975
#, no-c-format
msgid "#shadow#"
msgstr ""

#. Tag: para
#: Excel.xml:1978
#, no-c-format
msgid "Toggles shadowed font, use another #shadow# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:1986
#, no-c-format
msgid "#strikethrough#"
msgstr ""

#. Tag: para
#: Excel.xml:1989
#, no-c-format
msgid "Toggles strikethrough font, use another #strikethrough# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:1997
#, no-c-format
msgid "#subscript#"
msgstr ""

#. Tag: para
#: Excel.xml:2000
#, no-c-format
msgid "Toggles subscripted font, use another #subscript# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:2008
#, no-c-format
msgid "#superscript#"
msgstr ""

#. Tag: para
#: Excel.xml:2011
#, no-c-format
msgid "Toggles superscript font, use another #superscript# to turn it off"
msgstr ""

#. Tag: para
#: Excel.xml:2019
#, no-c-format
msgid "#font_name#"
msgstr ""

#. Tag: para
#: Excel.xml:2022
#, no-c-format
msgid "Sets font name, used like #font_name=Verdana\""
msgstr ""

#. Tag: para
#: Excel.xml:2029
#, no-c-format
msgid "#font_size#"
msgstr ""

#. Tag: para
#: Excel.xml:2032
#, no-c-format
msgid "Sets font size, use like #font_size=12#"
msgstr ""

#. Tag: programlisting
#: Excel.xml:2040
#, no-c-format
msgid ""
"<![CDATA[\r\n"
"            <e:workbook>\r\n"
"               <e:worksheet>         \r\n"
"                  <e:header>\r\n"
"                     <f:facet name=\"left\">\r\n"
"                        This document was made on #date# and has "
"#total_pages# pages\r\n"
"                     </f:facet>\r\n"
"                     <f:facet name=\"right\">\r\n"
"                        #time#\r\n"
"                     </f:facet>\r\n"
"                  </e:header>\r\n"
"               <e:worksheet>\r\n"
"            </e:workbook>\r\n"
"         ]]>"
msgstr ""

#. Tag: title
#: Excel.xml:2058
#, no-c-format
msgid "Print areas and titles"
msgstr ""

#. Tag: para
#: Excel.xml:2059
#, no-c-format
msgid ""
"Print areas and titles childrens of worksheets and worksheet templates and "
"provide... print areas and titles."
msgstr ""

#. Tag: literal
#: Excel.xml:2071
#, no-c-format
msgid "&lt;e:printArea&gt;"
msgstr ""

#. Tag: para
#: Excel.xml:2080
#, no-c-format
msgid ""
"<literal>firstColumn</literal> &#8212;The column of the top-left corner of "
"the area. The parameter is a number. Note that the value is 0-based."
msgstr ""

#. Tag: para
#: Excel.xml:2088
#, no-c-format
msgid ""
"<literal>firstRow</literal> &#8212;The row of the top-left corner of the "
"area. The parameter is a number. Note that the value is 0-based."
msgstr ""

#. Tag: para
#: Excel.xml:2096
#, no-c-format
msgid ""
"<literal>lastColumn</literal> &#8212;The column of the bottom-right corner "
"of the area. The parameter is a number. Note that the value is 0-based."
msgstr ""

#. Tag: para
#: Excel.xml:2104
#, no-c-format
msgid ""
"<literal>lastRow</literal> &#8212;The row of the bottom-right corner of the "
"area. The parameter is a number. Note that the value is 0-based."
msgstr ""

#. Tag: programlisting
#: Excel.xml:2137
#, no-c-format
msgid ""
"<![CDATA[\r\n"
"            <e:workbook>\r\n"
"               <e:worksheet>            \r\n"
"                  <e:printTitles firstRow=\"0\" firstColumn=\"0\" \r\n"
"                     lastRow=\"0\" lastColumn=\"9\"/>\r\n"
"                  <e:printArea firstRow=\"1\" firstColumn=\"0\" \r\n"
"                     lastRow=\"9\" lastColumn=\"9\"/>\r\n"
"               </e:worksheet>\r\n"
"            </e:workbook>\r\n"
"         ]]>"
msgstr ""

#. Tag: para
#: Excel.xml:2149
#, no-c-format
msgid "defines a print title between A1:A10 and a print area between B2:J10."
msgstr ""

#. Tag: title
#: Excel.xml:2154
#, no-c-format
msgid "Worksheet Commands"
msgstr ""

#. Tag: para
#: Excel.xml:2155
#, no-c-format
msgid ""
"Worksheet commands are children of workbooks and are usually executed only "
"once."
msgstr ""

#. Tag: title
#: Excel.xml:2160
#, no-c-format
msgid "Grouping"
msgstr ""

#. Tag: para
#: Excel.xml:2161
#, no-c-format
msgid "Provides grouping of columns and rows."
msgstr ""

#. Tag: literal
#: Excel.xml:2170
#, no-c-format
msgid "&lt;e:groupRows&gt;"
msgstr ""

#. Tag: para
#: Excel.xml:2179
#, no-c-format
msgid ""
"<literal>startRow</literal> &#8212;The row to start the grouping at. The "
"value is a number. Note that the value is 0-based."
msgstr ""

#. Tag: para
#: Excel.xml:2187
#, no-c-format
msgid ""
"<literal>endRow</literal> &#8212;The row to end the grouping at. The value "
"is a number. Note that the value is 0-based."
msgstr ""

#. Tag: para
#: Excel.xml:2195 Excel.xml:2260
#, no-c-format
msgid ""
"<literal>collapse</literal> &#8212;Should the grouping be collapsed "
"initially? The value is a boolean."
msgstr ""

#. Tag: emphasis
#: Excel.xml:2203 Excel.xml:2268 Excel.xml:2337 Excel.xml:2424
#, no-c-format
msgid "Child elements"
msgstr ""

#. Tag: literal
#: Excel.xml:2235
#, no-c-format
msgid "&lt;e:groupColumns&gt;"
msgstr ""

#. Tag: para
#: Excel.xml:2244
#, no-c-format
msgid ""
"<literal>startColumn</literal> &#8212;The column to start the grouping at. "
"The value is a number. Note that the value is 0-based."
msgstr ""

#. Tag: para
#: Excel.xml:2252
#, no-c-format
msgid ""
"<literal>endColumn</literal> &#8212;The column to end the grouping at. The "
"value is a number. Note that the value is 0-based."
msgstr ""

#. Tag: programlisting
#: Excel.xml:2293
#, no-c-format
msgid ""
"<![CDATA[\r\n"
"               <e:workbook>\r\n"
"                  <e:worksheet>            \r\n"
"                     <e:groupRows startRow=\"4\" endRow=\"9\" collapse=\"true"
"\"/>\r\n"
"                     <e:groupColumns startColumn=\"0\" endColumn=\"9\" "
"collapse=\"false\"/>\r\n"
"                  </e:worksheet>\r\n"
"               </e:workbook>\r\n"
"            ]]>"
msgstr ""

#. Tag: para
#: Excel.xml:2303
#, no-c-format
msgid ""
"groups rows 5 trough 10 and columns 5 through 10 so that the rows are "
"initially collapsed (but not the columns)."
msgstr ""

