#: Excel.xml:2324
#, no-c-format
msgid "&lt;e:link&gt;"
msgstr ""

#. Tag: para
#: Excel.xml:2333
#, no-c-format
msgid "<literal>URL</literal> &#8212;The URL to the stylesheet"
msgstr ""

#. Tag: programlisting
#: Excel.xml:2364
#, no-c-format
msgid ""
"<![CDATA[\n"
"               <e:workbook>\n"
