#, no-c-format
msgid "antlr.jar"
msgstr "antlr.jar"

#. Tag: literal
#: Websphere.xml:862
#, no-c-format
msgid "cglib.jar"
msgstr "cglib.jar"

#. Tag: literal
#: Websphere.xml:866
#, no-c-format
msgid "asm.jar"
msgstr "asm.jar"

#. Tag: literal
#: Websphere.xml:870
#, no-c-format
msgid "dom4j.jar"
msgstr "dom4j.jar"

#. Tag: literal
#: Websphere.xml:875
#, no-c-format
