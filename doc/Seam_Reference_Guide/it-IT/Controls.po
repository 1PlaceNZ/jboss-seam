"         <ui:insert name=\"label\"/>\n"
"         <s:span styleClass=\"required\" rendered=\"#{required}\">*</s:span>\n"
"      </s:label>\n"
"        \n"
"      <span class=\"#{invalid?'error':''}\">\n"
"         <s:validateAll>\n"
"            <ui:insert/>\n"
"         </s:validateAll>\n"
"      </span>\n"
"        \n"
"      <s:message styleClass=\"error\"/>     \n"
"      \n"
"   </div>   \n"
"  \n"
"</ui:composition>]]>"

