"   \n"
"   <ui:entity-converter name=\"restrictedEntityConverter\" entity-loader=\"#{restrictedEntityLoader}\" />\n"
"            \n"
"   <ui:jpa-entity-loader name=\"restrictedEntityLoader\" entity-manager=\"#{restrictedEntityManager}\" />]]>"

