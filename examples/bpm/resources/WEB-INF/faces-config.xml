<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config 
PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                            "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>

	<application>
		<variable-resolver>org.jboss.seam.SeamJSFVariableResolver</variable-resolver>
	</application>

	<navigation-rule>
		<description>Simple navigation rule.</description>
		<from-view-id>/register.jsp</from-view-id>
		<navigation-case>
			<description>Simple navigation case.</description>
			<from-outcome>register</from-outcome>
			<to-view-id>/registered.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
