<components>

    <component name="org.jboss.seam.core.init">
        <property name="debug">true</property>
        <property name="myFacesLifecycleBug">@myFacesLifecycleBug@</property>
    </component>
    
    <component name="themeSelector">
        <property name="availableThemes">default, accessible, printable</property>
    </component>
    
    <component name="entityManager" 
              class="org.jboss.seam.core.ManagedPersistenceContext">
        <property name="persistenceUnitJndiName">java:/blogEntityManagerFactory</property>
    </component>
    
    <component class="org.jboss.seam.core.Ejb" 
           installed="@embeddedEjb@"/>

</components>