<?xml version="1.0" encoding="UTF-8"?>
<pages xmlns="http://jboss.com/products/seam/pages"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://jboss.com/products/seam/pages file:///Users/pmuir/workspace/jboss-seam/src/main/org/jboss/seam/pages-2.0.xsd">

	<page view-id="/index.xhtml">
         <action execute="#{exel.init}"/>
