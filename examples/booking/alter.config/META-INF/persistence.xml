<persistence>
   <persistence-unit name="bookingDatabase">
      <provider>org.hibernate.ejb.HibernatePersistence</provider>
      <jta-data-source>java:/bookingDatasource</jta-data-source>
      <properties> 
         <property name="hibernate.hbm2ddl.auto" value="none"/>
      </properties>
   </persistence-unit>
</persistence>
