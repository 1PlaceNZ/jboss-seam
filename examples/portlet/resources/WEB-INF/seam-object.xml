<?xml version="1.0" encoding="UTF-8"?>
<deployments>
   <deployment>
         <if-exists>overwrite</if-exists>
         <parent-ref>default</parent-ref>
         <page>
            <page-name>Seam</page-name>
            <properties><property>
                  <name>org.jboss.portal.property.layout</name>
                  <value>generic</value>
               </property>
               <property>
                  <name>org.jboss.portal.property.theme</name>
                  <value>industrial</value>
               </property>
            </properties>
            <window>
               <window-name>MiniPortletWindow</window-name>
               <instance-ref>MiniPortletInstance</instance-ref>
               <region>center</region>
               <height>0</height>
            </window>
         </page>
   </deployment>
   <deployment>
      <if-exists>overwrite</if-exists>
      <instance>
         <instance-name>MiniPortletInstance</instance-name>
         <component-ref>MiniPortlet</component-ref>
      </instance>
   </deployment>
</deployments>


